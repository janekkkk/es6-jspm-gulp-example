{
  "name": "es6-jspm-gulp-example",
  "version": "0.3.1",
  "description": "Boilerplate for ES6+ apps using JSPM + Babel",
  "main": "dist/index.html",
  "homepage": "https://github.com/janekkkk/es6-jspm-gulp-example#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janekkkk/es6-jspm-gulp-example.git"
  },
  "bugs": "https://github.com/janekkkk/es6-jspm-gulp-example/issues",
  "author": {
    "name": "Alex Weber",
    "email": "alexweber15@gmail.com",
    "url": "http://alexweber.com.br"
  },
  "contributors": [
    {
      "name": "Janek Ozga",
      "email": "ikben@janekozga.nl"
    }
  ],
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "preinstall": "npm install -g npm jspm gulp browser-sync",
    "install": "jspm install && composer install",
    "test": "karma start",
    "test:watch": "karma start --no-single-run",
    "build": "gulp build",
    "docs": "npm run sassdocs && npm run jsdocs",
    "start": "gulp",
    "buildjs": "gulp buildjs",
    "jsdocs": "esdoc -c .esdoc.json",
    "sassdocs": "sassdoc src/scss"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "boilerplate",
    "example",
    "babel",
    "systemjs"
  ],
  "directories": {
    "doc": "docs",
    "test": "test",
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "browser-sync": "^2.11.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "del": "^1.2.0",
    "esdoc": "^0.2.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-config-standard": "^4.1.0",
    "eslint-plugin-react": "^3.15.0",
    "eslint-plugin-standard": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cached": "^1.1.0",
    "gulp-change": "^1.0.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^2.2.0",
    "gulp-connect-php": "0.0.7",
    "gulp-eslint": "^1.0.0",
    "gulp-foreach": "^0.1.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-intercept": "^0.1.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-minify-css": "^1.1.6",
    "gulp-minify-html": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-sass": "^2.0.1",
    "gulp-scss-lint": "^0.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.7",
    "imagemin-pngquant": "^4.1.0",
    "jspm": "^0.16.19",
    "karma": "^0.12.36",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jspm": "^2.0.2",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-sinon-chai": "^1.0.0",
    "mocha": "^2.2.5",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "sassdoc": "^2.1.15"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib",
      "test": "test"
    },
    "dependencies": {
      "jquery": "npm:jquery@^2.2.0",
      "lodash": "npm:lodash@^3.10.1",
      "zepto": "npm:zepto@^1.0.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
